# Staging Environment Configuration
VITE_APP_ENV=staging
VITE_APP_NAME="Blackjack Strategy Simulator (Staging)"
VITE_APP_VERSION="1.0.0-staging"
VITE_APP_URL=https://staging-blackjack-simulator.netlify.app

# API Configuration
VITE_API_BASE_URL=https://staging-api.blackjack-simulator.com/api
VITE_API_TIMEOUT=30000

# Analytics and Monitoring (staging environment)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=staging

# Feature Flags (staging settings)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_DEBUG_MODE=false

# PWA Configuration
VITE_PWA_CACHE_NAME=blackjack-simulator-staging
VITE_PWA_CACHE_VERSION=v1-staging

# Development Tools (disabled in staging)
VITE_ENABLE_STORYBOOK=false
VITE_ENABLE_DEV_TOOLS=false

# Build Configuration
VITE_BUILD_TARGET=es2015
VITE_BUNDLE_ANALYZER=false