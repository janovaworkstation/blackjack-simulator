# Development Environment Configuration
VITE_APP_ENV=development
VITE_APP_NAME="Blackjack Strategy Simulator (Dev)"
VITE_APP_VERSION="1.0.0-dev"
VITE_APP_URL=http://localhost:5173

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Analytics and Monitoring (disabled in dev)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# Feature Flags (development settings)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_DEBUG_MODE=true

# PWA Configuration
VITE_PWA_CACHE_NAME=blackjack-simulator-dev
VITE_PWA_CACHE_VERSION=v1-dev

# Development Tools
VITE_ENABLE_STORYBOOK=true
VITE_ENABLE_DEV_TOOLS=true

# Build Configuration
VITE_BUILD_TARGET=es2015
VITE_BUNDLE_ANALYZER=false